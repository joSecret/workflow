  <style>
    .span2 {
      --item-span: 2;
    }

    .span3 {
      --item-span: 3;
    }

    .description {
      grid-column: 1 / -1;
      display: grid;
      grid-template-rows: var(--description--gtr, 0fr);
      transition: grid-template-rows 0.5s;

      & > div {
        overflow: hidden;
      }
    }

    .item:hover + .description {
      --description--gtr: 1fr;
    }

    .grid {
      display: grid;
      grid-auto-flow: row dense;
      grid-template-columns: repeat(6, 1fr);
      grid-column-gap: 2rem;
      /* grid-row-gap: 1rem; */
    }

    .item {
      grid-column: span var(--item-span, 1);
      align-items: center;
      background-color: var(--item-bg, #fff);
      border-radius: 1.25rem;
      box-shadow: 6px 6px 16px 6px rgba(0, 0, 0, 0.08);
      display: flex;
      justify-content: center;
      padding: 1rem;
      margin-bottom: 1rem;
    }

    .green {
      --item-bg: #549b35;
    }

    .bluelight {
      --item-bg: #23b3d3;
    }

    .yallow {
      --item-bg: #fabf13;
    }

    .orange {
      --item-bg: #f3982d;
    }

    .brown {
      --item-bg: #be8a2f;
    }

    .blue {
      --item-bg: #0b97db;
    }
  </style>

  <style>
    main {
      width: 600px;
    }

    .card {
      background-color: #f5f5f5;
      border-radius: 1.25rem;
      padding: 1.5rem;
      margin-bottom: 1rem;

      & > *:first-child {
        margin-top: 0px;
      }

      & > *:last-child {
        margin-bottom: 0;
      }
    }
  </style>
</Layout>
